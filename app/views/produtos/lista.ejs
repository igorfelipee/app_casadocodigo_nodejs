<html>
	<head>
		<title>Lista de Produtos</title>
		<link rel="stylesheet" type="text/css" href="bootstrap.min.css">
	</head>
	<body>
		<table class="table table-striped">
			<tr>
				<td>ID</td>
				<td>Título</td>
				<td>Descrição</td>
				<td>Preço</td>
			</tr>
			<tr>
				<%for(var i=0;i<lista.length;i++){%>
					<tr>
						<td><%=lista[i].id%></td>
						<td><%=lista[i].titulo%></td>
						<td><%=lista[i].descricao%></td>
						<td><%=lista[i].preco%></td>
					</tr>
				<%}%>
			</tr>
		</table>
	</body>
</html>